{
  "name": "passport-starter",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.11.1",
    "@material-ui/icons": "^4.9.1",
    "chalk": "^4.1.0",
    "concurrently": "^5.3.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "http-proxy-middleware": "^1.0.6",
    "npm-run-all": "^4.1.5",
    "passport": "^0.4.1",
    "passport-facebook": "^3.0.0"
  },
  "scripts": {
    "start": "run-p start:** server",
    "start:app": "cd client && npm run start",
    "production": "run-p production:** server",
    "production:build": "cd client && npm run build",
    "server": "nodemon server.js",
    "dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "prod": "pm2 startOrReload prod-ecosystem.json --env production"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
